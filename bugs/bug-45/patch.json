{
  "apiVersion": "discovery.k8s.io/v1",
  "kind": "EndpointSlice",
  "metadata": {
    "name": "example-endpointslice",
    "namespace": "your-namespace"
  },
  "addressType": "IPv4",
  "ports": [
    {
      "name": "http",
      "protocol": "TCP",
      "port": 80
    }
  ],
  "endpoints": [
    {
      "addresses": [
        "192.168.1.1",
        "192.168.1.2"
      ],
      "ports": [
        {
          "name": "http",
          "protocol": "TCP",
          "port": 80
        }
      ],
      "topology": {
        "kubernetes.io/hostname": "node1"
      },
      "conditions": "ready"
    },
    {
      "addresses": [
        "192.168.1.3"
      ],
      "ports": [
        {
          "name": "http",
          "protocol": "TCP",
          "port": 80
        }
      ],
      "topology": {
        "kubernetes.io/hostname": "node2"
      }
    }
  ]
}