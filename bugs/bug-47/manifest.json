{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "test"
  },
  "spec": {
    "selector": {
      "matchLabels": {
        "app": "test"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "test"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "nginx",
            "image": "nginx:latest",
            "ports": [
              {
                "name": "http",
                "containerPort": 9090,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    }
  }
}
