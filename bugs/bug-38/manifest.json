{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "myapp",
    "namespace": "test"
  },
  "spec": {
    "selector": {
      "matchLabels": {
        "app": "myapp"
      }
    },
    "strategy": {
      "type": "Recreate"
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "myapp",
          "dummy": "dummy"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "myapp",
            "image": "nginx:1.15.8"
          }
        ]
      }
    }
  }
}
