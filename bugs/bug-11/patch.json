{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "test"
  },
  "spec": {
    "containers": [
      {
        "name": "regular-1",
        "image": "registry.k8s.io/e2e-test-images/busybox:1.29-4",
        "startupProbe": {
          "exec": {
            "command": [
              "/bin/false"
            ]
          },
          "failureThreshold": 1,
          "terminationGracePeriodSeconds": 1
        },
        "command": [
          "sh",
          "-c",
          "sleep 300s\n"
        ]
      }
    ],
    "terminationGracePeriodSeconds": 300
  }
}