{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "qos-demo-5"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app": "busybox"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "busybox"
        }
      },
      "spec": {
        "restartPolicy": "Always",
        "containers": [
          {
            "name": "qos-demo-ctr-5",
            "image": "docker.m.daocloud.io/library/nginx:1.14.2",
            "resources": {
              "limits": {
                "memory": "200Mi",
                "cpu": "700m"
              },
              "requests": {
                "memory": "200Mi",
                "cpu": "700m"
              }
            },
            "lifecycle": {
              "preStop": {
                "exec": {
                  "command": [
                    "/bin/sh",
                    "-c",
                    "sleep 10"
                  ]
                }
              }
            }
          }
        ]
      }
    }
  }
}